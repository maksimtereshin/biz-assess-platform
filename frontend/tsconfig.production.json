{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    /* Disable strict checks for production build */
    "strict": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "skipLibCheck": true,
    "allowJs": true
  }
}